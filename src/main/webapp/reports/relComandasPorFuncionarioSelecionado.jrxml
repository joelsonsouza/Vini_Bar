	<parameter name="funcionario" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT comanda.codigo,
	comanda.dtcomanda,
	comanda.hrreg,
	comanda.status,
	comanda.total,
	pessoa.nome
FROM comanda
	INNER JOIN funcionario ON 
	 comanda.funcionario_codigo = funcionario.codigo 
	INNER JOIN pessoa ON 
	 funcionario.pessoa_codigo = pessoa.codigo
	 WHERE 
	 comanda.dtcomanda BETWEEN $P{dtin} AND $P{dtfin} 
	 AND pessoa.nome = $P{funcionario} 
ORDER BY pessoa.nome DESC]]>
	</queryString>
