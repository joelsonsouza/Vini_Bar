	<queryString>
		<![CDATA[SELECT despesas.descricao,
	tipodespesa.periodo,
	tipodespesa.tipo,
	despesas.valor,
	despesas.dtdespesa,
	despesas.pago,
	CASE WHEN  despesas.pago = true THEN 'SIM' ELSE 'NÃO'
END
FROM despesas
	INNER JOIN tipodespesa ON 
	 despesas.tipodespesa_codigo = tipodespesa.codigo 
WHERE 
	 despesas.dtdespesa BETWEEN $P{dtin} AND $P{dtfin} 
	 AND tipodespesa.periodo = $P{mensalEventual} 
GROUP BY despesas.pago,
	despesas.dtdespesa,
	despesas.valor,
	tipodespesa.tipo,
	despesas.descricao,
	tipodespesa.periodo
ORDER BY tipodespesa.periodo DESC]]>
	</queryString>
